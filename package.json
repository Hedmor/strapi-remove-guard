{
  "name": "strapi-remove-guard",
  "version": "1.0.0",
  "description": "A simple utility for detecting removed fields in Strapi models.",
  "bin": {
    "sr-guard": "./bin/run"
  },
  "directories": {
    "lib": "src",
    "bin": "bin"
  },
  "files": [
    "dist",
    "bin"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Hedmor/strapi-remove-guard.git"
  },
  "scripts": {
    "build": "tsup-node",
    "build:watch": "tsup-node --watch",
    "clean": "rimraf dist",
    "commit": "cz",
    "commitlint": "commitlint --edit",
    "compile": "tsc",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "ts-node ./bin/run.ts",
    "start:node": "node ./bin/run",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepare": "husky",
    "release": "semantic-release"
  },
  "keywords": [
    "typescript",
    "starter",
    "cli",
    "bootstrap"
  ],
  "author": "Hedmor",
  "license": "UNLICENSED",
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@jest/globals": "^29.7.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.24",
    "@types/prompts": "^2.4.9",
    "@types/signale": "^1.4.7",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^3.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10",
    "semantic-release": "^23.1.1",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "consola": "^3.4.0",
    "dotenv": "^16.4.7",
    "giget": "^1.2.5",
    "picocolors": "^1.1.1",
    "simple-git": "^3.27.0",
    "yargs": "^17.7.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "packageManager": "pnpm@10.6.3+sha512.bb45e34d50a9a76e858a95837301bfb6bd6d35aea2c5d52094fa497a467c43f5c440103ce2511e9e0a2f89c3d6071baac3358fc68ac6fb75e2ceb3d2736065e6"
}
